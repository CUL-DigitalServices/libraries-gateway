<!-- Page header -->
<%- partials.header %>

<!-- Custom header -->
<div class="campl-row campl-page-header">
  <div class="campl-wrap clearfix campl-local-navigation" id="local-nav">
    <%- partials.navigation %>
  </div>
  <div class="campl-wrap clearfix campl-page-sub-title campl-recessed-sub-title">
    <div class="campl-column3 campl-spacing-column">&#160;</div>
    <div class="campl-column9">
      <div class="campl-content-container">
        <h2 class="campl-sub-title">Find a library</h2>
      </div>
    </div>
  </div>
</div>

<!-- Content -->
<div class="campl-row campl-content campl-recessed-content">
  <div class="campl-wrap clearfix">
    <div class="campl-column3">
      <div class="campl-tertiary-navigation lg-libraries-panel">
        <div class="campl-tertiary-navigation-structure">
          <div class="lg-filters">
            <div class="campl-search-input ls-search-input">
              <div class="lg-search-input-wrap">
                <input class="js-mini-search" type="text" placeholder="e.g. 'University Library'" />
              </div>
              <input type="image" class="campl-search-submit lg-search-submit" alt="Search" src="/public/images/interface/btn-search-inpage.png" />
            </div>

            <label>Filter libraries by:</label>

            <div class="btn-group lg-dropdown-selector lg-dropdown-selector-alphabet js-dropdown-selector-alphabet">
              <div class="btn btn-sm dropdown-toggle lg-dropdown-toggle" data-toggle="dropdown">
                <span class="js-value lg-dropdown-value" data-default-label="A-Z">A-Z</span>
                <button class="hidden lg-btn-remove js-remove"><span class="lg-icon-remove"></span></button>
              </div>
              <ul class="dropdown-menu">
                <li>
                  <a href="#" class="js-option" data-value="a">A</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="b">B</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="c">C</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="d">D</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="e">E</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="f">F</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="g">G</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="h">H</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="i">I</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="j">J</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="k">K</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="l">L</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="m">M</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="n">N</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="o">O</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="p">P</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="q">Q</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="r">R</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="s">S</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="t">T</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="u">U</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="v">V</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="w">W</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="x">X</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="y">Y</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="z">Z</a>
                </li>
              </ul>
            </div>

            <div class="btn-group lg-dropdown-selector js-dropdown-selector-area hidden">
              <div class="btn btn-sm dropdown-toggle lg-dropdown-toggle" data-toggle="dropdown">
                <span class="js-value lg-dropdown-value" data-default-label="Distance">Distance</span>
                <button class="hidden lg-btn-remove js-remove"><span class="lg-icon-remove"></span></button>
              </div>
              <ul class="dropdown-menu">
                <li>
                  <a href="#" class="js-option" data-value="0.25">Within 1/4 mile</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="0.5">Within 1/2 mile</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="1">Within 1 mile</a>
                </li>
                <li>
                  <a href="#" class="js-option" data-value="2">Within 2 miles</a>
                </li>
              </ul>
            </div>
          </div>
          <div class="list-group lg-libraries js-libraries" data-libraries="<%= data.libraries %>"></div>
        </div>
      </div>
    </div>
    <div class="campl-column9">
      <div class="js-maps-container maps-container"></div>
    </div>
  </div>
</div>
